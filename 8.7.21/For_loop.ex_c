
#include<stdio.h>
int main(void){
  int i;
  printf("\n using Increment\n");
  for(i=1; i<=10; i++)
    printf("%d",i);

    printf("\n using decrement \n");
    for(i=10; i>=1; i--)
    printf("%d", i);

    printf("\n using negative terms\n");
    for(i=-10;i<=0;i++)
    printf("%d",i);

    return 0;
}
